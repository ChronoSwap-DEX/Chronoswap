{
  "name": "my-dapp-template",
  "version": "0.1.0",
  "license": "GPL",
  "scripts": {
    "build": "next build",
    "clean:unix": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf dist",
    "clean:windows": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist dist rmdir /Q /S dist",
    "devnet:start": "npx --yes @alephium/cli@latest devnet start",
    "devnet:stop": "npx --yes @alephium/cli@latest devnet stop",
    "compile": "npx --yes @alephium/cli@latest compile",
    "deploy": "npx --yes @alephium/cli@latest deploy",
    "lint": "next lint",
    "lint:fix": "eslint . --fix --ext ts",
    "test": "jest -i --config ./jest-config.json",
    "dev": "next dev",
    "start": "next start",
    "build:scripts": "tsc --outDir ./dist/ scripts/*.ts"
  },
  "dependencies": {
    "@alephium/cli": "^0.29.3",
    "@alephium/token-list": "^0.0.14",
    "@alephium/web3": "^0.29.3",
    "@alephium/web3-react": "^0.29.3",
    "@alephium/web3-test": "^0.29.3",
    "@alephium/web3-wallet": "^0.29.3",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@reduxjs/toolkit": "^2.2.1",
    "ethers": "^5.7.2",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.7.2",
    "lodash-es": "^4.17.21",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^16.18.23",
    "@types/react": "18.2.56",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSameLine": false,
    "trailingComma": "none"
  }
}
